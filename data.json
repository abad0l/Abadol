  "marketing": {
    "title": "Marketing",
    "icon": "fas fa-chart-line",
    "description": "Healthcare marketing strategies and principles",
    "lectures": {
      "healthcare_marketing": {
        "title": "Healthcare Marketing",
        "icon": "fas fa-chart-line",
        "description": "Marketing principles applied to healthcare services",
        "quiz": [
          {
            "question": "What is the primary focus of healthcare marketing?",
            "options": {
              "A": "Maximizing profits",
              "B": "Building patient relationships and improving health outcomes",
              "C": "Reducing healthcare costs",
              "D": "Increasing medication sales"
            },
            "correctAnswer": "B",
            "explanation": "The primary focus of healthcare marketing is building patient relationships and improving health outcomes, rather than simply maximizing profits."
          },
          {
            "question": "Which marketing approach emphasizes understanding patient needs and delivering value?",
            "options": {
              "A": "Product-oriented marketing",
              "B": "Sales-oriented marketing",
              "C": "Market-oriented marketing",
              "D": "Production-oriented marketing"
            },
            "correctAnswer": "C",
            "explanation": "Market-oriented marketing focuses on understanding patient needs and delivering value that meets those needs, rather than simply pushing products or services."
          }
        ],
        "explanation": "\n                            <h3>Healthcare Marketing Overview</h3>\n                            <p>Healthcare marketing involves planning and executing the conception, pricing, promotion, and distribution of health services to create exchanges that satisfy individual and organizational objectives.</p>\n                            <h3>Unique Aspects of Healthcare Marketing</h3>\n                            <p>Healthcare marketing differs from other industries due to several factors:</p>\n                            <ul>\n                                <li><strong>Intangibility</strong>: Services cannot be seen or touched before purchase</li>\n                                <li><strong>Inseparability</strong>: Services are produced and consumed simultaneously</li>\n                                <li><strong>Variability</strong>: Service quality can vary between providers and encounters</li>\n                                <li><strong>Perishability</strong>: Services cannot be stored for future use</li>\n                                <li><strong>Regulatory environment</strong>: Strict regulations govern healthcare marketing</li>\n                            </ul>\n                            <h3>Marketing Mix in Healthcare (7 Ps)</h3>\n                            <p>The traditional marketing mix is expanded for services like healthcare:</p>\n                            <ol>\n                                <li><strong>Product</strong>: Healthcare services, treatments, and programs</li>\n                                <li><strong>Price</strong>: Cost of services, insurance coverage, and payment options</li>\n                                <li><strong>Place</strong>: Location and accessibility of healthcare facilities</li>\n                                <li><strong>Promotion</strong>: Communication about services through various channels</li>\n                                <li><strong>People</strong>: Healthcare providers and staff who deliver services</li>\n                                <li><strong>Process</strong>: Procedures and systems for delivering care</li>\n                                <li><strong>Physical evidence</strong>: Facility appearance, equipment, and materials</li>\n                            </ol>\n                            <h3>Ethical Considerations</h3>\n                            <p>Healthcare marketing must adhere to ethical principles:</p>\n                            <ul>\n                                <li>Truthfulness in all communications</li>\n                                <li>Respect for patient privacy and confidentiality</li>\n                                <li>Avoidance of fear-based marketing tactics</li>\n                                <li>Transparency about costs and outcomes</li>\n                                <li>Compliance with regulatory requirements</li>\n                            </ul>\n                            <h3>Digital Marketing in Healthcare</h3>\n                            <p>Digital channels have become increasingly important in healthcare marketing:</p>\n                            <ul>\n                                <li>Website and search engine optimization</li>\n                                <li>Social media engagement</li>\n                                <li>Email marketing for patient education and retention</li>\n                                <li>Online reputation management</li>\n                                <li>Telehealth and virtual care options</li>\n                            </ul>\n                        ",
        "flipCards": [
          {
            "front": "What are the 7 Ps of healthcare marketing?",
            "back": "Product, Price, Place, Promotion, People, Process, and Physical evidence."
          },
          {
            "front": "What makes healthcare marketing different from other industries?",
            "back": "Intangibility, inseparability, variability, perishability, and a strict regulatory environment."
          },
          {
            "front": "What is the primary focus of healthcare marketing?",
            "back": "Building patient relationships and improving health outcomes."
          }
        ]
      },
      "patient_engagement": {
        "title": "Patient Engagement",
        "icon": "fas fa-users",
        "description": "Strategies for involving patients in their healthcare",
        "quiz": [
          {
            "question": "What is patient engagement?",
            "options": {
              "A": "Patients paying their bills on time",
              "B": "Patients actively participating in their healthcare decisions",
              "C": "Patients following doctor's orders without question",
              "D": "Patients attending all scheduled appointments"
            },
            "correctAnswer": "B",
            "explanation": "Patient engagement refers to patients actively participating in their healthcare decisions and management, collaborating with providers to achieve better health outcomes."
          },
          {
            "question": "Which strategy is most effective for improving patient engagement?",
            "options": {
              "A": "Using complex medical terminology",
              "B": "Providing clear, understandable health information",
              "C": "Limiting patient access to medical records",
              "D": "Discouraging questions during appointments"
            },
            "correctAnswer": "B",
            "explanation": "Providing clear, understandable health information is one of the most effective strategies for improving patient engagement, as it empowers patients to participate meaningfully in their care."
          }
